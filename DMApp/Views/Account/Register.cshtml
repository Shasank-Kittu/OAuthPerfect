@model UserManager.Controllers.RegisterModel

@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Register</h2>


@using (Html.BeginForm("Register", "Account", FormMethod.Post))
{
    @Html.ValidationSummary()

    @Html.LabelFor(x => x.Username) @Html.TextBoxFor(x => x.Username)
    <br />
    @Html.LabelFor(x => x.password) @Html.PasswordFor(x => x.password)

    <br />
    @Html.LabelFor(x => x.Name) @Html.TextBoxFor(x => x.Name)
    <br />
    @Html.LabelFor(x => x.Gender) @Html.TextBoxFor(x => x.Gender)
    <br />
    <input type="submit" value="Register" />
}